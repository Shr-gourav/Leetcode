#include<iostream>
using namespace std;

int factorial(int n){
    int sum  = 1;
    while (n>0)
    {
        sum = sum * n;
        n--;
    }
    return sum;
}
int main(){
    int num;
    cout<<"Enter Number : "<<endl;
    cin>>num;
    cout<<"Factorial of "<<num<<" is "<<factorial(num)<<endl;
    return 0;
    
}
